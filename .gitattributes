# prevent eol hell
* text=auto !eol
*.php	text
*.inc	text
*.sql	text
*.js	text
*.css	text
*.po	text
*.png	binary
*.gif	binary
*.jpg	binary
*.ico	binary
*.mo	binary

# improve diff chunks headers
*.php	diff=php
*.inc	diff=php
.gitattributes export-ignore  
.gitignore export-ignore
.project export-ignore
.buildpath export-ignore
.travis.yml export-ignore
gulpfile.js export-ignore
package.json export-ignore
modules/tests export-ignore
